<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.silvertierai.scheduler</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/python3</string>
        <string>/Users/Shared/silvertierai/scheduler.py</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>
    
    <key>WorkingDirectory</key>
    <string>/Users/Shared/silvertierai</string>
    
    <key>StandardOutPath</key>
    <string>/Users/Shared/silvertierai/Logs/scheduler_stdout.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/Shared/silvertierai/Logs/scheduler_stderr.log</string>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>PYTHONPATH</key>
        <string>/Users/Shared/silvertierai</string>
    </dict>
</dict>
</plist>